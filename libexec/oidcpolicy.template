path "secret/oauth-<issuer>/creds/{{identity.entity.aliases.<oidc>.name}}:<role>" {
    capabilities = [ "list", "read", "update" ]
}
path "secret/oauth/creds/<issuer>/{{identity.entity.aliases.<oidc>.name}}:<role>" {
    capabilities = [ "update" ]
    allowed_parameters = {
        "server" = ["<issuer>"]
        "*"      = []
    }
}
path "secret/oauth/creds/<issuer>/{{identity.entity.aliases.<oidc>.name}}:<role>" {
    capabilities = [ "list", "read" ]
}
path "secret/oauth-<issuer>/creds/{{identity.entity.aliases.<oidc>.name}}/*" {
    capabilities = [ "list", "read", "update" ]
}
path "secret/oauth/creds/<issuer>/{{identity.entity.aliases.<oidc>.name}}/*" {
    capabilities = [ "update" ]
    allowed_parameters = {
        "server" = ["<issuer>"]
        "*"      = []
    }
}
path "secret/oauth/creds/<issuer>/{{identity.entity.aliases.<oidc>.name}}/*" {
    capabilities = [ "list", "read" ]
}
